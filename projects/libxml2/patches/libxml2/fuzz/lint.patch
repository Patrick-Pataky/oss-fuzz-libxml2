--- /dev/null
+++ b/test/valid/parameter_entity.xml
@@ -0,0 +1,42 @@
+<?xml version="1.0"?>
+<!DOCTYPE document [
+<!ENTITY % common.att "
+  id ID #IMPLIED
+  class CDATA #IMPLIED
+  xml:lang CDATA #IMPLIED">
+<!ENTITY % block.content "(#PCDATA|p|div)*">
+<!ENTITY % inline.content "(#PCDATA|span|em|strong)*">
+<!ELEMENT document (header, body)>
+<!ATTLIST document %common.att;>
+<!ELEMENT header (title, subtitle?)>
+<!ATTLIST header %common.att;>
+<!ELEMENT title %inline.content;>
+<!ATTLIST title %common.att;>
+<!ELEMENT subtitle %inline.content;>
+<!ATTLIST subtitle %common.att;>
+<!ELEMENT body (div+)>
+<!ATTLIST body %common.att;>
+<!ELEMENT div %block.content;>
+<!ATTLIST div %common.att;>
+<!ELEMENT p %inline.content;>
+<!ATTLIST p %common.att;>
+<!ELEMENT span %inline.content;>
+<!ATTLIST span %common.att;>
+<!ELEMENT em %inline.content;>
+<!ATTLIST em %common.att;>
+<!ELEMENT strong %inline.content;>
+<!ATTLIST strong %common.att;>
+]>
+<document id="doc1" xml:lang="en">
+  <header>
+    <title>Parameter <em>Entity</em> Example</title>
+    <subtitle>Using DTD with parameter entities</subtitle>
+  </header>
+  <body>
+    <div id="section1">
+      <p>This is a <span class="important">demonstration</span> of parameter entities in DTDs.</p>
+      <p>They allow for <strong>reusable</strong> attribute and content definitions.</p>
+    </div>
+  </body>
+</document>
+
