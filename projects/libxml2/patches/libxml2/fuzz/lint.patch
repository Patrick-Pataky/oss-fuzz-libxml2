+<?xml version="1.0"?>
+<!DOCTYPE composite [
+<!ELEMENT composite (section1, section2)>
+<!ELEMENT section1 (persons*)>
+<!ELEMENT section2 (products*)>
+
+<!-- Person DTD -->
+<!ELEMENT persons (person+)>
+<!ELEMENT person (name, age, email?)>
+<!ELEMENT name (#PCDATA)>
+<!ELEMENT age (#PCDATA)>
+<!ELEMENT email (#PCDATA)>
+<!ATTLIST person id ID #REQUIRED>
+
+<!-- Product DTD -->
+<!ELEMENT products (product+)>
+<!ELEMENT product (title, price)>
+<!ELEMENT title (#PCDATA)>
+<!ELEMENT price (#PCDATA)>
+<!ATTLIST product 
+          code ID #REQUIRED
+          inStock (yes|no) "yes">
+]>
+<composite>
+  <section1>
+    <persons>
+      <person id="p1">
+        <name>John Smith</name>
+        <age>30</age>
+        <email>john@example.com</email>
+      </person>
+      <person id="p2">
+        <name>Jane Doe</name>
+        <age>28</age>
+      </person>
+    </persons>
+  </section1>
+  <section2>
+    <products>
+      <product code="pr1" inStock="yes">
+        <title>Product One</title>
+        <price>19.99</price>
+      </product>
+      <product code="pr2" inStock="no">
+        <title>Product Two</title>
+        <price>29.99</price>
+      </product>
+    </products>
+  </section2>
+</composite>
