+<?xml version="1.0"?>
+<!DOCTYPE root [
+<!ELEMENT root (ns1:element|ns2:element)*>
+<!ATTLIST root
+          xmlns:ns1 CDATA #FIXED "http://example.org/namespace1"
+          xmlns:ns2 CDATA #FIXED "http://example.org/namespace2">
+<!ELEMENT ns1:element (ns1:child*)>
+<!ATTLIST ns1:element
+          id ID #REQUIRED
+          ns1:attr CDATA #IMPLIED>
+<!ELEMENT ns1:child (#PCDATA)>
+<!ELEMENT ns2:element (ns2:child*)>
+<!ATTLIST ns2:element
+          id ID #REQUIRED
+          ns2:attr CDATA #IMPLIED>
+<!ELEMENT ns2:child (#PCDATA)>
+]>
+<root xmlns:ns1="http://example.org/namespace1" 
+      xmlns:ns2="http://example.org/namespace2">
+  <ns1:element id="e1" ns1:attr="value1">
+    <ns1:child>Child of namespace 1</ns1:child>
+  </ns1:element>
+  <ns2:element id="e2" ns2:attr="value2">
+    <ns2:child>Child of namespace 2</ns2:child>
+  </ns2:element>
+</root>
